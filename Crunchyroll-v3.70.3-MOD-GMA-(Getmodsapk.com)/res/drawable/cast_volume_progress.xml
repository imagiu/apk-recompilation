<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="2.0dip" android:color="#42000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:drawable="@drawable/seekbar_secondary" />
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="left" android:clipOrientation="horizontal">
            <shape android:shape="line">
                <stroke android:width="2.0dip" android:color="@color/seek_bar_progress" />
            </shape>
        </clip>
    </item>
</layer-list>